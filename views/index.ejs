<% include partials/head %>

    <body>
        <header>
            <nav class="navbar navbar-inverse navbar-static-top" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav">
                            <li>
                                <a href="/">Home</a>
                            </li>
                            <li>
                                <a href="https://jobs.github.com/">GitHub Jobs</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>


        <div class="three-quarter-width center-block container-fluid">

            <main>
                <div class="col-md-6 panel panel-default">
                    <fieldset>
                        <div class="panel-heading">
                            <h1>What type of jobs are you looking for?</h1>
                        </div>
                        <div class="panel-body">

                                <form action="#" method="POST">
                                    <div class="bucket description">
                                        <div class="field">
                                            <label for="description_field" class="placeholder ghost-input">Enter in title, benefits, companies, or expertise</label>
                                            <input id="description_field" name="description" class="ghost-input" type="text" value="" />
                                        </div>
                                    </div>
                        </div>

                        <button type="button" id="findSomeJobs" class="btn btn-success ghost-button">Find Me a Job!</button>
                        <button type="button" id="clear-button" class="btn btn-info">Reset</button>
                        </form>
                </div>

                <div id="jobs-list-panel" class="col-md-6 panel panel-default">
                        <div class="panel-heading">
                            <h1 id="job-list">Job Listings</h1>
                        </div>
                        <div class="panel-body">
                            <ul id="job-results" class="list-group">
                            </ul>
                        </div>
                    </div>

        </div>


        </fieldset>


        <div class="col-md-12 panel panel-default">
            <div class="panel-heading" tabindex="0">
                <h1 id="job-details">Job Details</h1>
            </div>
            <div class="panel-body image" id="job-info">

            </div>

        </div>

        </main>

        </div>
  


        <% include partials/foot %>